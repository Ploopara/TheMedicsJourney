newGame(){
  resetScreen
  bootScreen
  export LEVELSAVE="0"
  writeSaveData
  loadLevel 1
}

writeSaveData() {
  spinCounter "Writing Save Data "
  echo "LEVELSAVE=\"$LEVELSAVE\"" > ~/.medicsjourney/savestate
  if isSet "$LVL1_CHS1_ANS"
  then
    echo "LVL1_CHS1_ANS=\"$LVL1_CHS1_ANS\"" >> ~/.medicsjourney/savestate
  fi
}
