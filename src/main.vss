include string

main(){
  trap quit INT # Quits cleanly rather than crashing out on ctrl-c
  mkdir -p ~/.medicsjourney
  bootScreenShort
  clear
  mainMenu
}

quit(){
  clear
  if [[ "$LEVELSAVE" != "0" && "$LEVELSAVE" != "" ]]; then
    writeSaveData
  fi
  echo "${colorsCyan}Goodbye!${colorsReset}"
  tput cnorm
  exit
}

isSet(){
    if [ "$1" == "" ]
    then
      return 1
    else
      return 0
    fi
}
